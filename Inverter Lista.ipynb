    "def inverter(lista):\n",
    "    aux = []\n",
    "    i = len(lista)-1\n",
    "\n",
    "    while i >= 0:\n",
    "        aux.append(lista[i])\n",
    "        i = i-1\n",
    "    return aux\n",
    "\n",
    "inverter([4,5,6]) # [4,3,2,1]\n",
    "    "
