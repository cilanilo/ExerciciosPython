    "def contrario(palavra):\n",
    "    aux = str()\n",
    "    i = len(palavra)-1\n",
    "    \n",
    "    while i >= 0:\n",
    "        aux = aux + palavra[i]\n",
    "        i = i - 1\n",
    "        \n",
    "    return aux\n",
    "\n",
    "contrario(\"diogo\") # ogoid\n",
    "    "
   ]
